<h1>Agregar Art√≠culos</h1>

<%= form_with model: @article do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.label :price %><br>
    <%= form.number_field :price %>
  </div>

  <div>
    <%= form.label :image %><br>
    <%= form.text_area :image %>
  </div>

  <div>
    <%= form.submit %>
  </div>

<% end %>
